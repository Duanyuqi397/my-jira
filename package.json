    "@welldone-software/why-did-you-render": "^6.2.3",
    "json-server": "^0.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx}": "prettier --write",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "msw": {
    "workerDirectory": "public"
